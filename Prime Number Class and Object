
class fact:
    def result(self):
        num=int(input("Enter a number"))
        cnt=0
        i=2
        while(i<=num/2):
            if(num % i == 0):
                cnt += 1
                i+=1
            else:
                i+=1
        if cnt > 0 :    
            print(num,"is not a prime number")
        else:
            print(num,"is a prime number")
obj=fact()
obj.result()
